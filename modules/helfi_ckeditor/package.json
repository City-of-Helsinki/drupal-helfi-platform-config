    "@ckeditor/ckeditor5-dev-utils": "^53.2.0",
    "@ckeditor/ckeditor5-icons": "^45.2.0",
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.1",
    "ckeditor5": "~35.1.0",
    "css-loader": "^6.8.1",
    "raw-loader": "^4.0.2",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.4.0"
  }
