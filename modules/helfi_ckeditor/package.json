{
  "name": "helfi-ckeditor",
  "version": "1.0.0",
  "description": "Helfi specific enhancements to CKEditor5 plugins.",
  "scripts": {
    "watch": "webpack --mode development --watch",
    "build": "npm run format:js --loglevel=silent && npm run lint:js --loglevel=silent && webpack",
    "lint:js": "biome lint assets/js/ckeditor5_plugins/",
    "format:js": "biome format --write assets/js/ckeditor5_plugins/"
  },
  "@comment dependencies": [
    "The following packages were updated manually.",
    "Remove this section once the dependencies have been updated in their respective upstream packages.",
    {
      "diff": "^8.0.3, dependency of ckeditor5-dev"
    }
  ],
  "overrides": {
    "diff": "^8.0.3"
  },
  "dependencies": {
    "@biomejs/biome": "^2.3.4",
    "@ckeditor/ckeditor5-core": "^47.3.0",
    "@ckeditor/ckeditor5-dev-translations": "^53.2.0",
    "@ckeditor/ckeditor5-dev-utils": "^53.2.0",
    "@ckeditor/ckeditor5-icons": "^47.3.0",
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.1",
    "ckeditor5": "~35.1.0",
    "css-loader": "^6.8.1",
    "raw-loader": "^4.0.2",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.4.0"
  }
}
