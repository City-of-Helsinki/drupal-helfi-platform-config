{
  "name": "helfi-etusivu-news-search",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "eslint-config-react-app": "7.0.1",
    "hds-react": "2.3.0",
    "luxon": "3.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.3.0"
  },
  "scripts": {
    "start": "BROWSER=none react-app-rewired start",
    "build": "react-app-rewired build && mv build/static/js/main.*.js ../assets/js/filter-events.js",
    "":"",
    "test": "TZ=UTC react-app-rewired test",
    "lint":"eslint --rule 'no-console: error'",
    "eject": "react-app-rewired eject",
    "prepare": "cd ../../../ && husky install helfi_features/helfi_events/js/husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/luxon": "^3.0.1",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.1.5",
    "husky": ">=4",
    "lint-staged": ">=10",
    "path-browserify": "^1.0.1",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd helf_features/helfi_events/js && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,css}": [
      "eslint --rule 'no-console: error'",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "<rootDir>/src/index.tsx",
      "<rootDir>/src/enum/*",
      "<rootDir>/src/types/*"
    ]
  }
}
