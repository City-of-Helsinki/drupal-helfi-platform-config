      path: modules/hdbt_admin_tools/hdbt_admin_tools.tokens.inc
      reportUnmatched: false
    -
      message: '#^Call to an undefined method Drupal\\Core\\Entity\\EntityInterface::getFileUri\(\).#'
      path: modules/hdbt_admin_tools/hdbt_admin_tools.tokens.inc
      reportUnmatched: false
